				validateDateRestriction: "Invalid date.",
				validateDateTimeRange: function(validationRoutineDetails) {
					var maxDate, minDate;				

					function defaultFormatter(dt){
						var dateParts = dt.split("-"),
							dateObj = new Date(dateParts[0],dateParts[1]-1,dateParts[2]),
							format;
							
						if(validationRoutineDetails.dateFormat && typeof($.datepicker)!='undefined'){
							format = DI.dateFormats[validationRoutineDetails.dateFormat];
						}
						
						return (format)?$.datepicker.formatDate(format,dateObj):dateParts[2] + '-' + dateParts[1] + '-' + dateParts[0];
					}
					if (validationRoutineDetails.maxDate) {
						maxDate = defaultFormatter(validationRoutineDetails.maxDate);
					}
					if (validationRoutineDetails.minDate) {
						minDate = defaultFormatter(validationRoutineDetails.minDate);
					}
					if (maxDate && minDate) {
						return "Please ensure that the date you have entered is between {0} and {1}.".format(minDate, maxDate);
					}
					else if (maxDate && (minDate === undefined)) {
						return "Please ensure that the date you have entered is earlier than {0}.".format(maxDate);
					}
					else if (minDate && (maxDate === undefined)) {
					if (validationRoutineDetails.minDateinclusive && (validationRoutineDetails.minDateinclusive).toLowerCase()==="true")
						return "Please ensure that the date you have entered is earlier than {0}.".format(minDate);
					else
						return "Please ensure that the date you have entered is later than {0}.".format(minDate);
					}
					else {
						return "Invalid date. Please ensure that the date you have entered is within the given range.";
					}
				},
